{% extends "LVBundle::layout.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {#{% stylesheets#}      {#filter='cssrewrite' %}#}      {#<link rel="stylesheet" href="{{ asset_url }}"/>#}      {#{% endstylesheets %}#}  {% endblock %}{% block lv_body %}    <div class="container" id="profile">        <div class="row profile">            <div class="col-sm-12">                {% for type, messages in app.session.flashBag.all %}                    {% for message in messages %}                        <div class="{{ type }} alert alert-danger">                            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span                                        aria-hidden="true">&times;</span></button>                            {{ message|trans({}, 'FOSUserBundle') }}                        </div>                    {% endfor %}                {% endfor %}            </div>            {% if app.user is defined and app.user %}                <div class="col-md-3">                    <div class="profile-sidebar">                        <!-- SIDEBAR USERPIC -->                        <div class="profile-userpic">                            <img src="http://ssl.gstatic.com/accounts/ui/avatar_2x.png"                                 class="img-responsive" alt="">                        </div>                        <!-- END SIDEBAR USERPIC -->                        <!-- SIDEBAR USER TITLE -->                        <div class="profile-usertitle">                            <div class="profile-usertitle-name">                                {{ app.user.username | capitalize }}                            </div>                            <div class="profile-usertitle-job">                                {{ app.user.email }}                            </div>                        </div>                        <!-- END SIDEBAR USER TITLE -->                        <!-- SIDEBAR BUTTONS -->                        <div class="profile-userbuttons">                            <button type="button" class="btn btn-success btn-sm"><i class="fa fa-video-camera"></i> Mes                                videos                            </button>                            <a href="{{ path('fos_user_security_logout') }}" class="btn btn-danger btn-sm"><i                                        class="fa fa-power-off"></i> DÃ©connexion                            </a>                        </div>                        <!-- END SIDEBAR BUTTONS -->                        <!-- SIDEBAR MENU -->                        <div class="profile-usermenu">                            <ul class="nav">                                <li {% if app.request.requestUri == path('fos_user_profile_show') %} class="active"{% endif %}>                                    <a href="{{ path('fos_user_profile_show') }}">                                        <i class="glyphicon glyphicon-user"></i>                                        Profil </a>                                </li>                                <li {% if app.request.requestUri == path('fos_user_profile_edit') %} class="active"{% endif %}>                                    <a href="{{ path('fos_user_profile_edit') }}">                                        <i class="fa fa-edit"></i>                                        Editer Profil </a>                                </li>                                <li {% if app.request.requestUri == path('fos_user_change_password') %} class="active"{% endif %}>                                    <a href="{{ path('fos_user_change_password') }}">                                        <i class="fa fa-key"></i>                                        Mot de passe </a>                                </li>                                <li {% if app.request.requestUri == path('list_video') %} class="active"{% endif %}>                                    <a href="{{ path('list_video') }}">                                        <i class="fa fa-list"></i>                                        Mes Videos </a>                                </li>                                <li {% if app.request.requestUri == path('report_video',{'id':app.user.id }) %} class="active"{% endif %}>                                    <a href="{{ path('report_video', {'id':app.user.id }) }}">                                        <i class="fa fa-flag"></i>                                        Signalements </a>                                </li>                                <li {% if app.request.requestUri == path('list_video') %} class="active"{% endif %}>                                    <a href="{{ path('list_video') }}">                                        <i class="fa fa-play"></i>                                        Mes Playlists </a>                                </li>                                <li>                                <li {% if app.request.requestUri == path('list_video') %} class="active"{% endif %}>                                    <a href="#">                                        <i class="fa fa-info"></i>                                        Aide </a>                                </li>                            </ul>                        </div>                        <!-- END MENU -->                    </div>                </div>            {% endif %}            <div {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %} class="col-sm-9" {% else %} class="col-sm-12" {% endif %}>                <div class="profile-content">                    {% block fos_user_content %}                    {% endblock fos_user_content %}                </div>            </div>        </div>    </div>{% endblock %}{#<div class="container">#}        {#<div class="row">#}            {#<div class="col-sm-12">#}                {#{% if is_granted("IS_AUTHENTICATE