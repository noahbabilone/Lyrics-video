{% extends "LVBundle::layout.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {#{% stylesheets#}      {#filter='cssrewrite' %}#}      {#<link rel="stylesheet" href="{{ asset_url }}"/>#}      {#{% endstylesheets %}#}  {% endblock %}{% block lv_body %}    <div class="container">        <div class="row">            <div class="col-sm-12">                {#<h3>Titre video</h3>#}                <hr>            </div>            <div class="col-sm-9">                <div class="row">                    <div class="col-sm-12" style="margin-bottom: 20px;" id="player">                        <div class="embed-responsive embed-responsive-16by9">                            <div class="" id="youtube-video"></div>                            <div class="sous-titre hide">Commencer la video pour afficher les sous-titres</div>                            <div id="data-video" data-video="{{ data }}"                                 style="display: none;">                            </div>                            {#{{ dump(data) }}#}                        </div>                    </div>                    <div class="col-sm-3">                        <a href="#" id="Signaler"                           class="btn btn-warning"> Signaler des fautes <i class="fa fa-fw" id="signal-icon"                                                                           aria-hidden="true"></i> (<span id="current">0:00</span>                            / <span id="duration">0:00</span>)</a>                    </div>                </div>                <div class="row">                    <div class="col-sm-12">                        <h3 class="page-header">Titre Video</h3>                    </div>                    <div class="col-sm-12">                        <div class="form-group">                            <label for="input-name">Nom</label>                            <input type="text" class="form-control" id="input-name" placeholder="Jane Doe">                        </div>                        <div class="form-group">                            <textarea class="form-control" id="textarea-comment" rows="3"></textarea>                        </div>                        <div class="form-group">                            <button type="submit" class="btn btn-default">Envoyer</button>                        </div>                    </div>                </div>            </div>            <div class="col-sm-3">                <div class="row">                    <div class="col-sm-12" style="margin-bottom: 12px;">                        <a href="#">                            <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">                        </a>                    </div>                    <div class="clearfix visible-xs-block"></div>                    <div class="col-sm-12" style="margin-bottom: 12px;">                        <a href="#">                            <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">                        </a>                    </div>                    <div class="col-sm-12" style="margin-bottom: 12px;">                        <a href="#">                            <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">                        </a>                    </div>                    <div class="col-sm-12" style="margin-bottom: 12px;">                        <a href="#">                            <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">                        </a>                    </div>                    <div class="col-sm-12" style="margin-bottom: 12px;">                        <a href="#">                            <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">                        </a>                    </div>                </div>            </div>        </div>    </div>{% endblock %}    {% block javascripts %}        {{ parent() }}        <script type="text/javascript" src="{{ asset('https://www.youtube.com/iframe_api') }}"></script>        <script type="text/javascript" src="{{ asset('bundle/lv/js/app.js') }}"></script>    {% endblock %}