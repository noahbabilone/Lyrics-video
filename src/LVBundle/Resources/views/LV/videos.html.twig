{% extends "LVBundle::layout.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {#{% stylesheets#}      {#filter='cssrewrite' %}#}      {#<link rel="stylesheet" href="{{ asset_url }}"/>#}      {#{% endstylesheets %}#}  {% endblock %}{% block lv_body %}    <div class="container">        <div class="row">            <div class="col-sm-12">                <h3 class="page-header">Vidéos                    <small>({{ videos | length }})</small>                </h3>            </div>            {#{{ dump(videos) }}#}            <div class="col-sm-12">                <div class="row">                    {% for video in videos %}                        <div class="col-sm-3">                            <a href="{{ path('watch_video',{'id':video.id}) }}">                                <div class="thumbnail">                                    <img src="https://i.ytimg.com/vi/{{ video.idVideo }}/mqdefault.jpg" alt="">                                    <div class="caption">                                        <h5>{{ video.title }}</h5>                                    </div>                                </div>                            </a>                        </div>                    {% else %}                        <p>Aucune vidéo disponible pour le moment</p>                    {% endfor %}                </div>            </div>        </div>    </div>{% endblock %}    {% block javascripts %}        {{ parent() }}    {% endblock %}