{% extends "LVBundle::layout.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {% stylesheets      'bundles/lv/cubeportfolio/css/cubeportfolio.css'      'bundles/lv/css/portfolio.min.css'      'bundles/lv/css/card.css'      filter='cssrewrite' %}      <link rel="stylesheet" href="{{ asset_url }}"/>      {% endstylesheets %}  {% endblock %}{% block lv_body %}    <div class="container">        <div class="row profile">            <div class="col-md-2">                {% include("LVBundle:Includes:menu.html.twig") %}            </div>            <div class="col-sm-10">                <div class="profile-content">                    <div class="col-sm-12">                        <h3 class="page-header text-primary">Vidéos                            <small class="pull-right">                                <ol class="breadcrumb">                                    <li><a href="#"><i class="fa fa-home"></i></a></li>                                    <li class="active">vidéos</li>                                </ol>                            </small>                        </h3>                    </div>                    {#{{ dump(videos) }}#}                    <div class="col-sm-12">                        <div class="portfolio-content portfolio-1">                            <div id="js-grid-juicy-projects" class="cbp">                                {#{% for i in 0..5 %}#}                                {% for video in videos %}                                    <div class="cbp-item" id="id-video-{{ video.id }}">                                        <article class="card">                                            <header class="card__thumb">                                                <a href="{{ path('watch_video',{'id':video.id}) }}">                                                    <img src="https://i.ytimg.com/vi/{{ video.idVideo }}/mqdefault.jpg"                                                         alt="">                                                </a>                                            </header>                                            <div class="card__body">                                                <h5 class="card__title"><a                                                            href="{{ path('watch_video',{'id':video.id}) }}">                                                        {% if video.title | length > 42 and app.user %}                                                            {% if app.user.id  == video.user.id %}                                                                {{ video.title | capitalize | slice(0, 37) ~ '...' }}                                                            {% else %}                                                                {{ video.title | capitalize | slice(0, 42) ~ '...' }}                                                            {% endif %}                                                        {% elseif video.title | length > 42 %}                                                            {{ video.title | capitalize | slice(0, 42) ~ '...' }}                                                        {% else %}                                                            {{ video.title | capitalize }}                                                        {% endif %}                                                    </a>                                                    {% if  app.user  and app.user.id  == video.user.id %}                                                        <span class="float">                                                               <a href="#suppimer" class="edit-video"><i                                                                           class="fa fa-edit text-primary"></i></a>                                                               <a href="#suppimer" class="supp-video"                                                                  data-id="video-{{ video.id }}"><i                                                                           class="fa fa-trash text-danger"></i></a>                                                            </span>                                                    {% endif %}                                                </h5>                                                <div class="card__description">                                                    <h5 class=""><a                                                                href="{{ path('watch_video',{'id':video.id}) }}">                                                            {{ video.title | capitalize }}</a>                                                    </h5>                                                    <i class="fa fa-users"></i>                                                    <small>{{ random(6) }}</small>                                                    &nbsp;                                                    <i class="fa fa-comments"></i>                                                    <small>{{ random(50) }}</small>                                                    {% if  app.user  and app.user.id  == video.user.id %}                                                        <span class="float">                                                               <a href="#suppimer" class="edit-video"><i                                                                           class="fa fa-edit text-primary"></i></a>                                                               <a href="#suppimer" class="btn-remove"                                                                  data-id="video-{{ video.id }}"><i                                                                           class="fa fa-trash text-danger"></i></a>                                                            </span>                                                    {% elseif video.user.username %}                                                        <span class="float">                                                               <small>                                                                   Publié(e)<b> {{ video.user.username | capitalize }}</b>                                                                 </small>                                                            </span>                                                    {% endif %}                                                </div>                                            </div>                                        </article>                                    </div>                                {% else %}                                    <p>Aucune vidéo disponible pour le moment</p>                                {% endfor %}                                {#{% endfor %}#}                            </div>                        </div>                    </div>                </div>            </div>        </div>    </div>    <div class="modal fade" tabindex="-1" id="modal-remove" role="dialog">        <div class="modal-dialog" role="document">            <!-- Modal content-->            <div class="modal-content">                <div class="modal-body">                    <p>                        Vous voulez vraiemnt supprimer cette vidéos (<span class="video-id-remove"></span>) ?                    </p>                </div>                <div class="modal-footer">                    <button type="button" class="btn btn-danger btn-ok-delete" data-dismiss="modal">Oui</button>                    <button type="button" class="btn btn-primary" data-dismiss="modal">Non</button>                </div>            </div>        </div>    </div>{% endblock %}  {% block javascripts %}    {{ parent() }}    {% javascripts    'bundles/lv/cubeportfolio/js/jquery.cubeportfolio.min.js'    'bundles/lv/js/portfolio-1.min.js'    'bundles/lv/js/applications.js' %}    <script src="{{ asset_url }}"></script>    {% endjavascripts %}{% endblock %}