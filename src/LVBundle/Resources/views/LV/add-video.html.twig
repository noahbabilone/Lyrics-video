{% extends "LVBundle::layout.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {#{% stylesheets#}      {#filter='cssrewrite' %}#}      {#<link rel="stylesheet" href="{{ asset_url }}"/>#}      {#{% endstylesheets %}#}  {% endblock %}{% block lv_body %}    <div class="container">        <div class="row">            <div class="col-sm-12">                {#<h3>Titre video</h3>#}                <hr>            </div>            <div class="col-sm-12">                <h3>Ajout</h3>            </div>            {% if form is defined %}                <div class="col-sm-12">                    {#{{ dump(adUser) }}#}                    {{ form_start(form, { 'method': 'POST','attr': { 'role':'form','class':'form-body'} }) }}                    <div class="row">                        <div class="col-sm-6">                            <div class="row">                                {% if form.title is defined %}                                    <div class="form-group has-feedback" id="field-name">                                        <div class="col-sm-8">                                            {{ form_label(form.title) }}                                        </div>                                        <div class="col-sm-4 ">                                            <div class="pull-right">                                                <button type="button" class="btn btn-primary btn-xs" id="btn-search"                                                        data-toggle="modal" {#data-target="#btn-search"#}>Rechercher                                                </button>                                            </div>                                        </div>                                        <div class="col-sm-12">                                            {{ form_widget(form.title) }}                                        </div>                                    </div>                                    <div class="col-sm-12">                                        <div id="youtube-video" class="">                                            <div id="youtube-video-player">                                            </div>                                        </div>                                        <div id="result-search"></div>                                    </div>                                {% endif %}                            </div>                        </div>                        <div class="col-sm-6">                            <div class="row">                                {% if form.subTitle is defined %}                                    <div class="col-sm-12" id="list-subtitle">                                        <div class="row"></div>                                    </div>                                    <div class="col-sm-12">                                        <a href="" class="btn btn-xs btn-success" id="btn-add-subtitle">Ajouter des                                            sous-titres</a>                                    </div>                                    <div class="col-sm-12 text-right">                                        <button type="submit" id="" class="btn btn-success btn-add-video hide">                                            Enregistrer la vid√©o                                        </button>                                    </div>                                    {{ form_widget(form.subTitle) }}                                {% endif %}                                <div class="hide " style="display: none;">                                    {% if form.idVideo is defined %}                                        {{ form_label(form.idVideo) }}                                        {{ form_widget(form.idVideo) }}                                    {% endif %}                                    {% if form.description is defined %}                                    {{ form_label(form.description) }}                                    {{ form_widget(form.description) }}                                </div>                                {% endif %}                            </div>                        </div>                    </div>                    {{ form_end(form) }}                </div>            {% endif %}        </div>    </div>    <div id="modal-search" class="modal" role="dialog">        <div class="modal-dialog modal-lg">            <div class="modal-content">                <div class="modal-header">                    <h4><i class="fa fa-film fa-fw"></i> Titre</h4>                </div>                <!-- Modal content-->                <div class="modal-body">                    <div class="row">                        <div class="col-sm-12">                        </div>                    </div>                </div>                <div class="modal-footer">                    <button type="button" class="btn btn-default" data-dismiss="modal">Fermer</button>                </div>            </div>        </div>    </div>{% endblock %}    {% block javascripts %}        {{ parent() }}        <script type="text/javascript" src="{{ asset('https://www.youtube.com/iframe_api') }}"></script>        <script type="text/javascript" src="{{ asset('bundles/lv/js/form.js') }}"></script>    {% endblock %}